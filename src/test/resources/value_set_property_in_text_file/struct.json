{
  "vertices": [
    {
      "label": "person",
      "input": {
        "type": "${source_type}",
        "path": "${store_path}/vertex_person.txt",
        "format": "TEXT",
        "header": ["name", "age", "city"],
        "charset": "UTF-8"
      },
      "field_mapping": {
        "name": "name",
        "age": "age",
        "city": "city"
      }
    },
    {
      "label": "software",
      "input": {
        "type": "${source_type}",
        "path": "${store_path}/vertex_software.txt",
        "header": ["name", "lang", "price"],
        "format": "TEXT"
      }
    }
  ],
  "edges": [
    {
      "label": "use",
      "source": ["person_name"],
      "target": ["software_name"],
      "input": {
        "type": "${source_type}",
        "path": "${store_path}/edge_use.txt",
        "header": ["person_name", "software_name", "feel", "time"],
        "format": "TEXT",
        "date_format": "yyyy-MM-dd"
      },
      "null_values": [""],
      "field_mapping": {
        "person_name": "name",
        "software_name": "name"
      }
    }
  ]
}
