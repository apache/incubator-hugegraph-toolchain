{
  "vertices": [
    {
      "label": "person",
      "input": {
        "type": "${source_type}",
        "path": "${store_path}/vertex_person.csv",
        "format": "CSV",
        "charset": "UTF-8"
      },
      "field_mapping": {
        "name": "name",
        "age": "age",
        "city": "city"
      }
    },
    {
      "label": "software",
      "input": {
        "type": "${source_type}",
        "path": "${store_path}/vertex_software.csv",
        "format": "CSV"
      }
    }
  ],
  "edges": [
    {
      "label": "knows",
      "source": ["source_name"],
      "target": ["target_name"],
      "input": {
        "type": "${source_type}",
        "path": "${store_path}/edge_knows.csv",
        "format": "CSV"
      },
      "field_mapping": {
        "source_name": "name",
        "target_name": "name"
      }
    },
    {
      "label": "created",
      "source": ["source_name"],
      "target": ["target_name"],
      "input": {
        "type": "${source_type}",
        "path": "${store_path}/edge_created.csv",
        "format": "CSV"
      },
      "field_mapping": {
        "source_name": "name",
        "target_name": "name"
      }
    }
  ]
}
