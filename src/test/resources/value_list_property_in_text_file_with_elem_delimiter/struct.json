{
  "vertices": [
    {
      "label": "person",
      "input": {
        "type": "${source_type}",
        "path": "${store_path}/vertex_person.txt",
        "format": "TEXT",
        "delimiter": "\t",
        "header": ["name", "age", "city"],
        "charset": "UTF-8",
        "list_format": {
          "start_symbol": "[",
          "end_symbol": "]",
          "elem_delimiter": ";"
        }
      },
      "field_mapping": {
        "name": "name",
        "age": "age",
        "city": "city"
      }
    }
  ]
}
